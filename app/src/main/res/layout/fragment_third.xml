        app:layout_constraintBottom_toTopOf="@+id/botonprevious2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textimpacto6"
        app:layout_constraintVertical_bias="0.02" />

    <Spinner
        android:id="@+id/spinnerdificultad2"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toTopOf="@+id/botonprevious2"
        app:layout_constraintEnd_toEndOf="@+id/textView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textdificultadretraso2"
        app:layout_constraintVertical_bias="0.003" />

    <Spinner
        android:id="@+id/spinnerimpacto2"
        android:layout_width="200dp"
