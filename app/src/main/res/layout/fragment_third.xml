        android:text="Impacto en Enfermedad por Retraso (6 semanas)"
        android:textColor="#100101"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/botonsig2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerdificultad2"
        app:layout_constraintVertical_bias="0.01" />

    <TextView
        android:id="@+id/textdificultadretraso2"
        android:layout_width="388dp"
        android:layout_height="58dp"
