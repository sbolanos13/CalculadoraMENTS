        android:text="Impacto en Dificultad/Riesgo intervención por Retraso (2 semanas)  "
        android:textColor="#0C0000"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/botonprevious2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.434"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerimpacto2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textimpacto2"
        android:layout_width="359dp"
        android:layout_height="56dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/fredoka_one"
        android:text="Impacto en Enfermedad por Retraso (2 semanas) "
        android:textColor="#100101"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.192"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerriesgo"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textriesgo"
        android:layout_width="391dp"
        android:layout_height="58dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/fredoka_one"
        android:text="Opción Tratamiento NO Operatorio (Riesgo Exposición)"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textview_second2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerefectividad"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/botonsig2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="84dp"
        android:layout_marginBottom="20dp"
        android:text="SIGUIENTE"
        android:textColor="#100000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
